<%= form_with(model: @event, local: true) do |form| %>

  <div class="title">
    <%= form.label "タイトル" %>
    <%= form.text_field :title %>
  </div>

    <div class="time">
    <%= form.label "日時" %>
    <%= form.date_select :start_time %>
  </div>

  <div class="content">
    <%= form.label "内容" %>
    <%= form.text_field :content %>
  </div>


  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>