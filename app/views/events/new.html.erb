  <div class="new-contents">
  <header class="new-header">
  <p>練習日誌を記載しよう！</p>
  </header>

<%= form_with(model: @event, local: true) do |form| %>

  <div class="title">
    <%= form.label "タイトル" %>
    <%= form.text_field :title %>
  </div>

    <div class="time">
    <%= form.label "日時" %>
    <%= form.date_select :start_time %>
  </div>

  <div class="content">
    <%= form.label "トレーニング内容" %>
    <%= form.text_field :content %>
  </div>


  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>